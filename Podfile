	pod 'IoniconsSwift'
	pod 'Alamofire'
	pod 'SwiftyJSON'
	pod 'Kingfisher', '~> 3.0'
	pod 'Firebase/Core'
	pod 'Firebase/Messaging'
	pod 'GooglePlaces'
  	pod 'GooglePlacePicker'
  	pod 'GoogleMaps'
  # Pods for GV24

  target 'GV24Tests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'GV24UITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
